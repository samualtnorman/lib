{
	"name": "@samual/lib",
	"version": "0.3.4",
	"main": "index.js",
	"private": true,
	"scripts": {
		"build": "rollup --config",
		"dev": "rollup --config --watch",
		"lint": "eslint .; tsc",
		"package": "rm -rf dist && rollup --config && npm run gen-types && node scripts/build-package-json && cp LICENSE README.md index.cjs dist",
		"gen-types": "tsc --project src --declaration true --emitDeclarationOnly true --noEmit false --outDir dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/samualtnorman/lib.git"
	},
	"author": "Samual Norman",
	"license": "GPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/samualtnorman/lib/issues"
	},
	"homepage": "https://github.com/samualtnorman/lib#readme",
	"engines": {
		"node": "^12.20 || >=14"
	},
	"types": "index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"import": "./index.js",
			"require": "./index.cjs"
		},
		"./*": "./*"
	},
	"files": [
		"*"
	],
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/preset-env": "^7.16.4",
		"@babel/preset-typescript": "^7.16.0",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-node-resolve": "^13.0.6",
		"@types/node": "^12.20.37",
		"@typescript-eslint/eslint-plugin": "^5.8.1",
		"@typescript-eslint/parser": "^5.8.1",
		"eslint": "^8.5.0",
		"eslint-plugin-array-func": "^3.1.7",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-optimize-regex": "^1.2.1",
		"eslint-plugin-regexp": "^1.5.1",
		"eslint-plugin-unicorn": "^42.0.0",
		"latest-version": "^6.0.0",
		"rollup": "^2.60.0",
		"rollup-plugin-preserve-shebang": "^1.0.1",
		"rollup-plugin-terser": "^7.0.2",
		"typescript": "^4.5.4"
	},
	"optionalDependencies": {
		"deasync": "^0.1.24"
	}
}
