{
	"name": "@samual/lib",
	"version": "0.3.4",
	"main": "index.js",
	"private": true,
	"scripts": {
		"build": "rollup --config",
		"dev": "rollup --config --watch",
		"lint": "eslint .; tsc",
		"package": "rm -rf dist && rollup --config && npm run gen-types && node scripts/build-package-json && cp LICENSE README.md index.cjs dist",
		"gen-types": "tsc --project src --declaration true --emitDeclarationOnly true --noEmit false --outDir dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/samualtnorman/lib.git"
	},
	"author": "Samual Norman",
	"license": "GPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/samualtnorman/lib/issues"
	},
	"homepage": "https://github.com/samualtnorman/lib#readme",
	"engines": {
		"node": ">=14"
	},
	"types": "index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"import": "./index.js",
			"require": "./index.cjs"
		},
		"./*": "./*"
	},
	"files": [
		"*"
	],
	"devDependencies": {
		"@babel/core": "^7.17.10",
		"@babel/preset-env": "^7.17.10",
		"@babel/preset-typescript": "^7.16.7",
		"@rollup/plugin-babel": "^5.3.1",
		"@rollup/plugin-commonjs": "^22.0.0",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@types/node": "^14.18.18",
		"@typescript-eslint/eslint-plugin": "^5.23.0",
		"@typescript-eslint/parser": "^5.23.0",
		"eslint": "^8.15.0",
		"eslint-plugin-array-func": "^3.1.7",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-optimize-regex": "^1.2.1",
		"eslint-plugin-regexp": "^1.7.0",
		"eslint-plugin-unicorn": "^42.0.0",
		"latest-version": "^6.0.0",
		"rollup": "^2.72.1",
		"rollup-plugin-preserve-shebang": "^1.0.1",
		"rollup-plugin-terser": "^7.0.2",
		"semver": "^7.3.7",
		"typescript": "^4.6.4"
	},
	"optionalDependencies": {
		"deasync": "^0.1.26"
	}
}
