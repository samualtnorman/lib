{
	"name": "@samual/lib",
	"version": "0.7.0",
	"main": "index.js",
	"private": true,
	"scripts": {
		"lint": "eslint .; tsc; tsc --project src",
		"build": "rollup --config",
		"dev": "pnpm build --watch",
		"emit-declarations": "tsc --project src --declaration --emitDeclarationOnly --noEmit false --outDir dist",
		"package": "rm -rf dist && pnpm build && pnpm emit-declarations && scripts/build-package-json.js && cp LICENSE README.md index.cjs dist",
		"update": "pnpm update --latest !@types/node && pnpm update @types/node"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/samualtnorman/lib.git"
	},
	"author": "Samual Norman",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/samualtnorman/lib/issues"
	},
	"homepage": "https://github.com/samualtnorman/lib#readme",
	"engines": {
		"node": ">=16"
	},
	"types": "index.d.ts",
	"type": "module",
	"exports": {
		"./fetch": {
			"browser": "./fetch/browser.js",
			"default": "./fetch/index.js"
		},
		".": {
			"import": "./index.js",
			"require": "./index.cjs"
		},
		"./*": "./*.js",
		"./*.js": "./*.js"
	},
	"files": [
		"*"
	],
	"devDependencies": {
		"@babel/core": "^7.22.5",
		"@babel/preset-env": "^7.22.5",
		"@babel/preset-typescript": "^7.22.5",
		"@babel/types": "^7.22.5",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-node-resolve": "^15.1.0",
		"@rollup/plugin-terser": "^0.4.3",
		"@samual/lib": "^0.7.0",
		"@types/node": "^16.18.36",
		"@typescript-eslint/eslint-plugin": "^5.59.11",
		"@typescript-eslint/parser": "^5.59.11",
		"eslint": "^8.42.0",
		"eslint-plugin-array-func": "^3.1.8",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-regexp": "^1.15.0",
		"eslint-plugin-unicorn": "^47.0.0",
		"magic-string": "^0.30.0",
		"rollup": "^3.25.1",
		"semver": "^7.5.1",
		"typescript": "^5.1.3"
	},
	"optionalDependencies": {
		"deasync": "^0.1.28",
		"node-fetch": "^3.3.1"
	}
}
