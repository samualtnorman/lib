{
	"name": "@samual/lib",
	"version": "0.4.0",
	"main": "index.js",
	"private": true,
	"scripts": {
		"build": "rollup --config",
		"dev": "rollup --config --watch",
		"lint": "eslint .; tsc",
		"package": "rm -rf dist && rollup --config && npm run gen-types && node scripts/build-package-json && cp LICENSE README.md index.cjs dist",
		"gen-types": "tsc --project src --declaration true --emitDeclarationOnly true --noEmit false --outDir dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/samualtnorman/lib.git"
	},
	"author": "Samual Norman",
	"license": "GPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/samualtnorman/lib/issues"
	},
	"homepage": "https://github.com/samualtnorman/lib#readme",
	"engines": {
		"node": ">=14"
	},
	"types": "index.d.ts",
	"type": "module",
	"exports": {
		"./fetch": {
			"browser": "./fetch/browser.js",
			"default": "./fetch/index.js"
		},
		".": {
			"import": "./index.js",
			"require": "./index.cjs"
		},
		"./*": "./*.js",
		"./*.js": "./*.js"
	},
	"files": [
		"*"
	],
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.18.6",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^24.0.1",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@types/node": "^14.18.36",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"eslint": "^8.34.0",
		"eslint-plugin-array-func": "^3.1.8",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-regexp": "^1.12.0",
		"eslint-plugin-unicorn": "^45.0.2",
		"latest-version": "^7.0.0",
		"rollup": "^3.17.1",
		"rollup-plugin-preserve-shebang": "^1.0.1",
		"rollup-plugin-terser": "^7.0.2",
		"semver": "^7.3.8",
		"typescript": "^4.9.5"
	},
	"optionalDependencies": {
		"deasync": "^0.1.28",
		"node-fetch": "^3.3.0"
	}
}
