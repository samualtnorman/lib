{
	"name": "@samual/lib",
	"version": "0.5.0",
	"main": "index.js",
	"private": true,
	"scripts": {
		"prepare": "ts-patch install -s",
		"lint": "eslint .; tsc; tsc --project src",
		"build": "rollup --config",
		"dev": "pnpm build --watch",
		"emit-declarations": "tsc --project src --declaration --emitDeclarationOnly --noEmit false --outDir dist",
		"package": "rm -rf dist && pnpm build && pnpm emit-declarations && scripts/build-package-json.js && cp LICENSE README.md index.cjs dist",
		"update": "pnpm update --latest !@types/node && pnpm update @types/node"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/samualtnorman/lib.git"
	},
	"author": "Samual Norman",
	"license": "GPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/samualtnorman/lib/issues"
	},
	"homepage": "https://github.com/samualtnorman/lib#readme",
	"engines": {
		"node": ">=14"
	},
	"types": "index.d.ts",
	"type": "module",
	"exports": {
		"./fetch": {
			"browser": "./fetch/browser.js",
			"default": "./fetch/index.js"
		},
		".": {
			"import": "./index.js",
			"require": "./index.cjs"
		},
		"./*": "./*.js",
		"./*.js": "./*.js"
	},
	"files": [
		"*"
	],
	"devDependencies": {
		"@babel/core": "^7.21.3",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.21.0",
		"@babel/types": "^7.21.3",
		"@rollup/plugin-alias": "^4.0.3",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-terser": "^0.4.0",
		"@samual/lib": "^0.5.0",
		"@types/node": "^14.18.38",
		"@typescript-eslint/eslint-plugin": "^5.55.0",
		"@typescript-eslint/parser": "^5.55.0",
		"eslint": "^8.36.0",
		"eslint-plugin-array-func": "^3.1.8",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-regexp": "^1.13.0",
		"eslint-plugin-unicorn": "^46.0.0",
		"magic-string": "^0.30.0",
		"rollup": "^3.19.1",
		"semver": "^7.3.8",
		"ts-patch": "^2.1.0",
		"typescript": "^5.0.2",
		"typescript-transform-paths": "^3.4.6"
	},
	"optionalDependencies": {
		"deasync": "^0.1.28",
		"node-fetch": "^3.3.1"
	}
}
