{
	"name": "@samual/lib",
	"version": "0.0.1",
	"main": "index.js",
	"scripts": {
		"build": "rollup --config && tsc --project src",
		"dev": "rollup --config --watch",
		"prepublishOnly": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/samualtnorman/lib.git"
	},
	"author": "Samual Norman",
	"license": "GPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/samualtnorman/lib/issues"
	},
	"homepage": "https://github.com/samualtnorman/lib#readme",
	"engines": {
		"node": "^12.20 || >=14"
	},
	"types": "index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"import": "./index.js",
			"require": "./index.cjs"
		},
		"./*": "./*"
	},
	"files": [
		"*.js",
		"*.d.ts",
		"!*.config.js",
		"index.cjs"
	],
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/preset-env": "^7.16.4",
		"@babel/preset-typescript": "^7.16.0",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-node-resolve": "^13.0.6",
		"@types/node": "^12.20.37",
		"rollup": "^2.60.0",
		"rollup-plugin-terser": "^7.0.2",
		"typescript": "^4.5.2"
	},
	"optionalDependencies": {
		"deasync": "^0.1.24"
	}
}
